<?xml version="1.0" encoding="utf-8"?>
<AmyInteractionModel>
	<Intent
		ref="de.unistuttgart.iaas.amyassist.amy.plugin.example.HelloWorldSpeech.say">
		<gram> say hello </gram>
	</Intent>

	<Intent
		ref="de.unistuttgart.iaas.amyassist.amy.plugin.example.HelloWorldSpeech.sayHelloXTimes">
		<gram> say hello {number} </gram>
		<EntityTemplates>
			<EntityTemplate id="number" type="int" />
		</EntityTemplates>

		<Prompt entityTemplateId="number">
			<text> how many times should i say hello? </text>
			<gram> {number} [times] </gram>
		</Prompt>
	</Intent>

	<Intent
		ref="de.unistuttgart.iaas.amyassist.amy.plugin.example.HelloWorldSpeech.getContacts">
		<gram>get contacts</gram>
	</Intent>

	<Intent
		ref="de.unistuttgart.iaas.amyassist.amy.plugin.example.HelloWorldSpeech.testContacts">
		<gram>test contacts registry</gram>
	</Intent>

	<Intent 
		ref="de.unistuttgart.iaas.amyassist.amy.plugin.example.HelloWorldSpeech.testLocation">
		<gram>test location registry</gram>
	</Intent>

	<Intent
		ref="de.unistuttgart.iaas.amyassist.amy.plugin.example.HelloWorldSpeech.testCustom">
		<gram>test custom registry</gram>
	</Intent>
</AmyInteractionModel>