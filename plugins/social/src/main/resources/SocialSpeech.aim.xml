<?xml version="1.0" encoding="utf-8"?>
<AmyInteractionModel>
	<Intent
		ref="de.unistuttgart.iaas.amyassist.amy.plugin.social.SocialSpeech.greeting">
		<gram>(hello|hi|good (morning|evening|day)) [amy]</gram>
	</Intent>
	<Intent
		ref="de.unistuttgart.iaas.amyassist.amy.plugin.social.SocialSpeech.userName">
		<gram>(i am | my name is) {name}</gram>
		<EntityTemplates>
			<EntityTemplate id="name" required="true">
				<gram>*</gram>
			</EntityTemplate>
		</EntityTemplates>
	</Intent>
	<Intent
		ref="de.unistuttgart.iaas.amyassist.amy.plugin.social.SocialSpeech.askForName">
		<gram>(what|whats) (is|s) your name</gram>
	</Intent>
	<Intent
		ref="de.unistuttgart.iaas.amyassist.amy.plugin.social.SocialSpeech.whatsUp">
		<gram>(what is| what s) (up|going on)</gram>
	</Intent>
	<Intent
		ref="de.unistuttgart.iaas.amyassist.amy.plugin.social.SocialSpeech.howAreYou">
		<gram>how (are you [feeling]| do you [feel])</gram>
	</Intent>
	<Intent
		ref="de.unistuttgart.iaas.amyassist.amy.plugin.social.SocialSpeech.sampleSentences">
		<gram>(what (can|may) i ask you [about] | help [me]) [[with] [the plugin] {pluginname}]</gram>
		<EntityTemplates>
			<EntityTemplate id="pluginname" required="false">
				<gram>*</gram>
			</EntityTemplate>
		</EntityTemplates>
	</Intent>
	<Intent
		ref="de.unistuttgart.iaas.amyassist.amy.plugin.social.SocialSpeech.getInstalledPlugins">
		<gram>(what plugins are installed | (tell me about your [installed] | installed) plugins)</gram>
	</Intent>
	<Intent
		ref="de.unistuttgart.iaas.amyassist.amy.plugin.social.SocialSpeech.tellMeAboutPlugin">
		<gram> tell me about {plugin} </gram>
		<EntityTemplates>
			<EntityTemplate id="plugin" required="true">
				<gram>*</gram>
			</EntityTemplate>
		</EntityTemplates>

		<Prompt entityTemplateId="plugin">
			<text> what plugin should i tell you about? </text>
			<gram> [about] {plugin} </gram>
		</Prompt>
	</Intent>
	<Intent
		ref="de.unistuttgart.iaas.amyassist.amy.plugin.social.SocialSpeech.tellJoke">
		<gram>(Tell me|say) (a joke|something funny)</gram>
	</Intent>
</AmyInteractionModel>